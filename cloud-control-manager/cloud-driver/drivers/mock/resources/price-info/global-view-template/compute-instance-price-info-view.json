{
    "meta": {
        "version": "v0.1",
        "description": "Multi-Cloud Price Info"
    },
    "cloudName": "%cloudName%",
    "regionName": "%regionName%",
    "zoneName": "%regionName%",
    "priceList": [
        {
            "productInfo": {
                "productId": "%productId%",
                "vmSpecInfo": {
                    "name": "%instanceType%",
                    "vCpu": {
                        "count": "%vcpu%"
                    },
                    "memSizeMiB": "%memory%",
                    "diskSizeGB": "%storage%",
                    "gpu": [
                        {
                            "count": "%gpu%",
                            "memSizeGB": "%gpuMemory%"
                        }
                    ]
                },
                "description": "%productInfo.description%",
                "cspProductInfo": "%cspProductInfo%"
            },
            "priceInfo": {
                "onDemand": {
                    "pricingId": "%pricingId%",
                    "unit": "%unit%",
                    "currency": "%currency%",
                    "price": "%price%",
                    "description": "%pricingPolicy.description%"
                },
                "cspPriceInfo": "%cspPriceInfo%"
            }
        }
    ]
}