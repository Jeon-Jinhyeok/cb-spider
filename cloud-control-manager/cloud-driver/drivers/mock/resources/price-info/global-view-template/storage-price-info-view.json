{
    "meta": {
        "version": "v0.1",
        "description": "Multi-Cloud Price Info"
    },
    "cloudPriceList": [
        {
            "cloudName": "%cloudName%",
            "priceList": [
                {
                    "productInfo": {
                        "productId": "%productId%",
                        "regionName": "%regionName%",
                        "zoneName": "%regionName%",
                        "volumeType": "%volumeType%",
                        "storageMedia": "%storageMedia%",
                        "maxVolumeSize": "%maxVolumeSize%",
                        "maxIopsvolume": "%maxIopsvolume%",
                        "maxThroughputvolume": "%maxThroughputvolume%",
                        "description": "%productInfo.description%",
                        "cspProductInfo": "%cspProductInfo%"
                    }, 
                    "priceInfo": {                        
                        "pricingPolicies": [
                            {
                                "pricingId": "%pricingId%",
                                "pricingPolicy": "%pricingPolicy%",
                                "unit": "%unit%",
                                "currency": "%currency%",
                                "price" : "%price%",
                                "description": "%pricingPolicy.description%",
                                "pricingPolicyInfo": {
                                    "LeaseContractLength": "%LeaseContractLength%",
                                    "OfferingClass": "%OfferingClass%",
                                    "PurchaseOption": "%PurchaseOption%"
                                }
                            }
                        ],
                        "cspPriceInfo": "%cspPriceInfo%"
                    }
                }
            ]
        }
    ]
}